{
    "manifest_version": 3,
    "name": "Application Tracking System",
    "version": "1.0",
    "description": "Chrome extension for the application tracking system website to autofill applications",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://127.0.0.1:5000/*",
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }